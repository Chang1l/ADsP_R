rm(list=ls())
m1=matrix(seq(1,10),nrow = 5,byrow = TRUE)

m2=matrix(seq(1,6),ncol=3)

m3=rbind(seq(1,3),seq(4,6),seq(7,12))

m4=cbind(seq(1,3),seq(4,6))

rm(list=ls())
m <- matrix(seq(1,12),nrow=4, ncol=3)

m1 <-m[2,3]
m2 <-m[3,] # 생략시 전체 행
m3 <-m[,-2] # 2행 제외 전체 row
m4 <- m[c(2,3), 2] #2번하고 3번행의 2열
m5 <- m[c(TRUE,FALSE,FALSE, TRUE),] #TRUE 행 열전체



rm(list=ls())

a1=array(1:12, dim=c(3,2,2)) #3행2열 구조 2개를 만들고 1부터 12값
a2=array(1:6, dim=c(2,3))# 2행 3열에 1부터 6값
